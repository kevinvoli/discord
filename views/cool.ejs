    </div>
    <div class="container-login100-form-btn m-t-20">
        <button class="login100-form-btn">
            Se connecter

        </button>
    </div>


<script>

    const socket= io('http://localhost:3000/register')
	$('.validate-form').on('submit',(e)=>{
		e.preventDefault()

		let	data={
				nom:$('#nom').val(),
				pseudo:$('#pseudo').val(),
				password:$('#password').val(),
				email:$('#email').val(),
				number:$('#number').val()
			}
		socket.emit('inscription',data)
	})	

	</script>